{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react-jsx",
    "strict": true,
    "types": ["react", "react-native"],
    "allowJs": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "lib": ["es2015", "dom", "dom.iterable", "es2017"]
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
